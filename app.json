{
  "expo": {
    "name": "JongyClientSide",
    "slug": "JongyClientSide",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/Jonggylogo.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/Jonggylogo.png",
      "resizeMode": "contain",
      "backgroundColor": "#2F2F2F"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow $(PRODUCT_NAME) to use your location."
        }
      ],
        ["expo-font",{
        "fonts":["/assets/fonts/Kanit-Regular.ttf"]}]
    ]
  }
}